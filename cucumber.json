{
  "default": {
    "format": [
      "html:test-results/cucumber-report.html",
      "progress-bar",
      "json:test-results/cucumber-report.json"
    ],
    "formatOptions": { "snippetInterface": "async-await" },
    "paths": ["src/test/features/**/"],
    "dryRun": false,
    "require": [
      "src/test/stepDefinitions/**/*.ts",
      "src/hooks/hooks.ts",
      "src/world/world.ts"
    ],
    "requireModule": ["ts-node/register"]
  }
}
